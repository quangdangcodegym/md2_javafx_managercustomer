// Neu can gan gia tri cho controller
FXMLLoader loader = new FXMLLoader(CustomerManager.class.getResource("detailcustomer.fxml"));
Parent root = loader.load();

((DetailCustomerController) loader.getController()).setModel(model);

Scene scene = new Scene(root);
Stage stage = new Stage();

stage.setTitle("Hello!");
stage.setScene(scene);
stage.show();

// KO can gan gia tri cho controller
FXMLLoader fxmlLoader = new FXMLLoader(HelloApplication.class.getResource("detailcustomer.fxml"));
Scene scene = new Scene(fxmlLoader.load());

// fxmlLoader.load() thuc thi se goi initialize cua controller
